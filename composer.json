{
  "name": "basduchambre/external-db-connect",
  "type": "library",
  "description": "Laravel package to connect to external database and fetch data.",
  "license": "MIT",
  "authors": [
    {
      "name": "Bastiaan van de Kamer",
      "email": "bas@duchambre.media"
    }
  ],
  "require": {
    "php": ">7.0"
  },
  "autoload": {
    "psr-4": {
      "Basduchambre\\ExternalDbConnect\\": "src/"
    }
  },
  "scripts": {
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Basduchambre\\ExternalDbConnect\\ExternalDbConnectServiceProvider"
      ],
      "aliases": {
        "ExternalDbConnect": "Basduchambre\\ExternalDbConnect\\Facade"
      }
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0"
  }
}
